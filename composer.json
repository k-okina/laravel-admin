{
    "name": "custom/encore/laravel-admin",
    "description": "custom laravel admin",
    "type": "library",
    "keywords": ["laravel", "admin", "grid", "chart", "form", "k-okina", "kahiro"],
    "homepage": "https://github.com/k-okina/laravel-admin",
    "license": "MIT",
    "authors": [
        {
            "name": "k-okina",
            "email": "okinakahiro@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/k-okina/laravel-admin"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "intervention/image" : "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "laravel/laravel": "5.2.*",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Admin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Models\\": "tests/models",
            "Tests\\Controllers\\": "tests/controllers"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
